/boot	emmc	/dev/block/by-name/boot
/cache	ext4	/dev/block/by-name/cache
/data	ext4	/dev/block/by-name/data	flags=encryptable=/factory/userdata_footer;length=-16384
/factory	emmc	/dev/block/by-name/factory	flags=display="Factory";backup=1
/misc	emmc	/dev/block/by-name/misc	flags=display="Misc";backup=1
/recovery	emmc	/dev/block/by-name/recovery	flags=display="Recovery";backup=1
/splashscreen	emmc	/dev/block/by-name/splashscreen	flags=display="Splashscreen";backup=1
/system	ext4	/dev/block/by-name/system
/external_sd	vfat	/dev/block/mmcblk1p1	flags=display="MicroSD";storage;wipeingui;removable
